class Program
{
	void main()
	{
		//declarations
		int sum=0;

		callout("print","Size of arrays: ");
		int n=read();

		int[n] ar1;
		int[n] ar2;
		int[n] arsum;


		//initializing vectors
		int  i;
		for i=0,n
		{			
			ar1[i] = read();
		}

		int  i;
		for i=0,n
		{			
			ar2[i] = read();			
		}

		//adding element-wise
		int  i;
		for i=0,n
		{
			arsum[i]=ar1[i]+ar2[i];
		}

		//printing answer
		int  k;
		for k=0,n
		{
			callout("print",arsum[k]);
		}
	}
}