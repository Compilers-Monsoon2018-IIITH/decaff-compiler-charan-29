class Program
{
	void main()
	{

		callout("print","Size of array: ");
		int n=read();
		int[n] ar;

		callout("print","Query number to search for: ");
		int q = read();

		//initializing array
		int i;
		for i=0,n
		{
			ar[i]=read();
		}

		//adding elements of array
		int j;
		for j=0,n
		{
			if(ar[j]==q)
			{
				callout("print","found",q);
				break;
			}
		}

	}
}